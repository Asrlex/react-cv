{
  "homepage": "https://asrlex.github.io/react-cv",
  "name": "react-cv",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "Alex Sanchez. <alexsruc@hotmail.com>",
  "description": "A modern CV built with React and Vite.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asrlex/react-cv.git"
  },
  "scripts": {
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/material": "^7.0.2",
    "@testing-library/react": "^16.3.0",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^25.1.3",
    "motion": "^12.6.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0",
    "react-photo-view": "^1.2.7",
    "react-scroll": "^1.9.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
